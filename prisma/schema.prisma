// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}
model Usuario {
  id Int @id @default(autoincrement())
  nome String
  email String @unique
  telefone String @unique
  senha String
  dataNascimento DateTime
  

  @@map("usuarios")
}
model Inquilino {
  id Int @id @default(autoincrement())
  nome String
  datanascimento DateTime
  cidade String
  tipoMoradia String
  seTrabalha String
  seEstuda String
 

  @@map("inquilinos")
 
}
model Imovel {
  id Int @id @default(autoincrement())
  nome String
  telefone String @unique
  cidade String
  bairro String
  cep String
  rua String
  caractImovel String
  defPessoas String
  valor Int
 

  @@map("imoveis")
}
